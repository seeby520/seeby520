    www.fill(0);
    qDebug()<<www;

    QString  ss =  "[sssss]";
    ss.remove("[");
    ss.remove("]");
    bool sss = false;
    int aa = sss;
    QUdpSocket udp;
  bool awwa =   udp.bind(QHostAddress("192.168.1.111"), 30333);

    static    int _prs_number = 0;
